{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write .",
    "generate-migration": "drizzle-kit generate",
    "up": "drizzle-kit up",
    "migrate": "tsx src/migrate",
    "dev": "tsx watch --clear-screen=false src/server.ts",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.30.10",
    "electric-sql": "^0.11.3",
    "fastify": "^4.26.2",
    "postgres": "^3.4.4",
    "tsx": "^4.7.2"
  },
  "devDependencies": {
    "@types/pg": "^8.11.4",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.21.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.4"
  }
}
